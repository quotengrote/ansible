version: "2.1"
services:
  blocky:
    image: spx01/blocky
    container_name: blocky
    restart: unless-stopped
    ports:
      - "53:53/tcp"
      - "53:53/udp"
    environment:
      TZ: Europe/Berlin
    volumes:
      # config file
      - ./config.yml:/app/config.yml
    labels:
      com.centurylinklabs.watchtower.enable: true

# kann mit dig @192.168.2.43 -p 54 alternate.de getestet werden
